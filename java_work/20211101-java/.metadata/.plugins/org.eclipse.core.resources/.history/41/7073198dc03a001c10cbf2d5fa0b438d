
public class ex06 {
	public static void main(String[] args) {
		System.out.println("MATH.PI"+Math.PI);
		
		System.out.println("Math.cos(30)"+Math.cos(30));
		System.out.println("Math.cos(30)"+Math.sin(30));
		
		System.out.println("Math.cos(30)"+Math.cos(0));
		System.out.println("Math.cos(30)"+Math.sin(0));


		System.out.println("Math.cos(30)"+Math.abs(-10));
	}
}
